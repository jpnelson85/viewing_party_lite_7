<p> "Welcome, <%= @user.user_name %>!" </p>

<h3> <%= "#{@user.user_name}'s Dashboard" %> </h3>

<p> <%= button_to "Discover Movies", "/users/#{@user.id}/discover", method: "get" %> </p>

<h4> List of Viewing Parties </h4>

<h2><% @user.parties.each do |party| %></h2>
  <p>Movie Title: <%= party.movie_title %></p>
  <p>Party Host: <%= party.host %></p>
  <img width='100px' src=<%= party.movie_image %>>
    <p>Attendees<% party.attendees.each do |invite| %></p>
      <p><%= invite.user_name %></p>
    <% end %>
<% end %>
